{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "core-js/es7/reflect": [
        "node_modules/core-js/proposals/reflect-metadata"
      ],
      "hal-render-component": [
        "dist/hal-render-component"
      ],
      "hal-render-component/*": [
        "dist/hal-render-component/*"
      ],
      "chrome-plugin": [
        "dist/chrome-plugin"
      ],
      "chrome-plugin/*": [
        "dist/chrome-plugin/*"
      ],
      "tree-model": [
        "dist/tree-model"
      ],
      "tree-model/*": [
        "dist/tree-model/*"
      ],
      "uri-template-editor": [
        "dist/uri-template-editor"
      ],
      "uri-template-editor/*": [
        "dist/uri-template-editor/*"
      ],
      "hal-form-template": [
        "dist/hal-form-template"
      ],
      "hal-form-template/*": [
        "dist/hal-form-template/*"
      ]
    }
  }
}